	<DataSources>
		<DataSource Name="Reels">
			<ConnectionProperties>
				<ConnectString>Data Source=..\\..\\..\\..\\..\\..\\..\\..\\Data\\Reels.db</ConnectString>
				<DataProvider>SQLITE</DataProvider>
			</ConnectionProperties>
		</DataSource>
	</DataSources>
	<DataSets>
		<DataSet Name="Genres">
			<Fields>
				<Field Name="GenreID">
					<DataField>GenreID</DataField>
				</Field>
				<Field Name="GenreName">
					<DataField>GenreName</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>SELECT GenreID, GenreName FROM Genre
ORDER BY 2</CommandText>
				<DataSourceName>Reels</DataSourceName>
			</Query>
		</DataSet>
		<DataSet Name="SaleDetails">
			<Fields>
				<Field Name="GenreID">
					<DataField>GenreID</DataField>
				</Field>
				<Field Name="GenreName">
					<DataField>GenreName</DataField>
				</Field>
				<Field Name="Quantity">
					<DataField>Quantity</DataField>
				</Field>
				<Field Name="SaleDate">
					<DataField>SaleDate</DataField>
				</Field>
				<Field Name="MonthYear">
					<DataField>MonthYear</DataField>
				</Field>
				<Field Name="Quarter">
					<Value>=Choose(  1+ (Fields!SaleDate.Value.Month - 1) / 3, "Q1", "Q2", "Q3", "Q4" )</Value>
				</Field>
			</Fields>
			<Query>
				<CommandText>="SELECT Genre.GenreID, Genre.GenreName, Sum(SaleDetails.Quantity) AS Quantity, First_Value(Sale.SaleDate) over (Order By Sale.SaleDate) AS SaleDate, strftime('%m/%Y',[SaleDate]) AS MonthYear FROM Sale INNER JOIN ((Product INNER JOIN (Genre INNER JOIN ((Movie INNER JOIN MovieProduct ON Movie.MovieID = MovieProduct.MovieID) INNER JOIN MovieGenres ON Movie.MovieID = MovieGenres.MovieID) ON Genre.GenreID = MovieGenres.GenreID) ON Product.ProductID = MovieProduct.ProductID) INNER JOIN SaleDetails ON Product.ProductID = SaleDetails.ProductID) ON Sale.SalesID = SaleDetails.SaleID " &amp; IIF(Parameters!AllGenres.Count &lt;&gt; Parameters!GenreID.Count, "WHERE Genre.GenreID IN (@GenreIds) ", "" ) &amp; " GROUP BY Genre.GenreID, Genre.GenreName, strftime('%m/%Y',[SaleDate]) "</CommandText>
				<DataSourceName>Reels</DataSourceName>
				<QueryParameters>
					<QueryParameter Name="@GenreIds">
						<Value>=Parameters!GenreID.Value</Value>
					</QueryParameter>
				</QueryParameters>
			</Query>
		</DataSet>
	</DataSets>
