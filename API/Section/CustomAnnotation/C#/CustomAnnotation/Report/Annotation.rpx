  <Script><![CDATA[public void ActiveReport_DataInitialize()
{
	rpt.Fields.Add("ExtendedPrice2");
}


public bool ActiveReport_FetchData(bool eof)
{
	rpt.Fields["ExtendedPrice2"].Value	= Convert.ToDouble(rpt.Fields["UnitPrice"].Value)	* Convert.ToDouble(rpt.Fields["Quantity"].Value)* (1 - Convert.ToDouble(rpt.Fields["Discount"].Value))* 1.05;
	return eof;
}


public void GFOrderID_BeforePrint()
{
	this.txtTotal.Value	= Convert.ToDouble(this.txtFreight.Value)+ Convert.ToDouble(this.txtSubTotal.Value);
}


public void Detail_BeforePrint()
{
	if (Convert.ToDouble(this.txtDiscount.Value) == 0)
	{
		this.txtDiscount.Text = "";
	}
}


